<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parkAndGo.gui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".IdentificationActivity"
            android:label="@string/title_activity_identification" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PaiementActivity"
            android:label="@string/title_activity_paiement" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                >
            </intent-filter>
        </activity>
        <activity
            android:name=".ProlongerActivity"
            android:label="@string/title_activity_prolonger" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                
            </intent-filter>
        </activity>
        <activity
            android:name=".HistoriqueActivity"
            android:label="@string/title_activity_historique" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                
            </intent-filter>
        </activity>
    </application>

</manifest>